{
  "assistant": {
    "default_model": {
      "provider": "ollama",
      "model": "llama3.1:latest"
    },
    "version": "2"
  },
  "vim_mode": true,
  "command_aliases": {
    "CLO": "pane: close inactive items",
    "ON": "workspace: close all docks",
    "q": "pane: close all items",
    "ERRS": "diagnostics: deploy",
    "qa": "zed: quit"
  },
  "git": {
    "inline_blame": { "enabled": false }
  },
  "buffer_font_weight": 501,
  "preferred_line_length": 120,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  }
}
